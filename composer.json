{
    "name": "voilaah/skillupdoc-plugin",
    "type": "october-plugin",
    "description": "Auto-Build and Display markdown files in the backend for documentation purposes",
    "require": {
        "composer/installers": "~1.0",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.8.1"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "homepage": "https://github.com/Voilaah/oc-skillupdoc-plugin.git",
    "keywords": ["october", "octobercms", "documentation", "markdown"]
}
